#!/usr/bin/env python
# TODO: This routine is still under development
import vplot as vpl
import argparse

if __name__ == "__main__":
    parser = argparse.ArgumentParser(prog="vplot", add_help=False)
    parser.add_argument(
        "-g",
        "--group",
        default="param",
        help="What to group plots by (param | type | none)",
    )
    args = parser.parse_args()

    vpl.auto_plot(group=args.group)
